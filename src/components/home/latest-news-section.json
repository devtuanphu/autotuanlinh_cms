{
  "collectionName": "components_home_latest_news_sections",
  "info": {
    "displayName": "Latest News Section",
    "description": "Section tin tức mới nhất với title, subtitle, view all button và danh sách blog",
    "icon": "newspaper"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": false,
      "default": "Tin tức mới nhất"
    },
    "subtitle": {
      "type": "text",
      "required": false
    },
    "viewAllButton": {
      "type": "component",
      "component": "shared.cta-button",
      "repeatable": false,
      "required": false
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "required": false
    }
  },
  "config": {}
}

